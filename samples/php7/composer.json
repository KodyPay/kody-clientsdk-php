{
    "name": "kody/kody-php7-grpc-sample",
    "description": "A PHP7 sample how to use Kody SDK PHP7 client to communicate with Kody terminals",
    "type": "project",
    "license": "MIT",
    "require": {
        "kody/kody-php7-grpc-client": "v1.4.*",
        "grpc/grpc": "^1.57",
        "google/protobuf": "^3.19.1"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "kody/kody-php7-grpc-client",
                "version": "v1.4.4",
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/KodyPay/kody-clientsdk-php/releases/download/v1.4.4/kody-php7-grpc-package.zip"
                }
            }
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Com\\Kodypay\\Grpc\\Pay\\V1\\": "vendor/kody/kody-php7-grpc-client/Com/Kodypay/Grpc/Pay/V1",
            "GPBMetadata\\": "vendor/kody/kody-php7-grpc-client/GPBMetadata"
        }
    }
}
