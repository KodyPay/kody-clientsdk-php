{
    "name": "kody/kody-php8-grpc-sample-ecom",
    "description": "A PHP8 sample how to use Kody SDK PHP client to make payments",
    "type": "project",
    "license": "MIT",
    "require": {
        "kody/kody-php8-grpc-client": "v1.2.2",
        "grpc/grpc": "^1.57",
        "google/protobuf": "^3.19.1",
        "vlucas/phpdotenv": "^4.3"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "kody/kody-php8-grpc-client",
                "version": "v1.2.2",
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/KodyPay/kody-clientsdk-php/releases/download/v1.2.2/kody-php8-grpc-package.zip"
                }
            }
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Com\\Kodypay\\Grpc\\Pay\\V1\\": "vendor/kody/kody-php8-grpc-client/Com/Kodypay/Grpc/Pay/V1",
            "GPBMetadata\\": "vendor/kody/kody-php8-grpc-client/GPBMetadata"
        }
    }
}
